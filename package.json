{
  "name": "yetitours",
  "version": "1.0.0",
  "engines": {
    "node": ">=8.3.0"
  },
  "description": "A web application for booking tours in nature",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon server.js",
    "start:prod": "set NODE_ENV=production&& nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js --public-url /js"
  },
  "author": "Dimitris Arabatzis",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^1.3.2",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.21.3",
    "hpp": "^0.2.3",
    "html-to-text": "^7.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.14",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.2",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.7",
    "sharp": "^0.30.5",
    "slugify": "^1.4.6",
    "stripe": "^8.137.0",
    "sweetalert": "^2.1.2",
    "validator": "^12.2.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1"
  }
}
